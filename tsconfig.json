{
  "compilerOptions": {
    "target": "ES2017",
    "module": "es2015",
    "sourceMap": true,
    "moduleResolution": "node",
    "outDir": "./public/js",
    "rootDir": "./src/tsc",
    "removeComments": true,
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "suppressImplicitAnyIndexErrors": true
